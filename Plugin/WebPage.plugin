[Rewrite]
# -----------Rucu6----------- #
# 桌面端请使用 https://google.com/ncr
# 谷歌服务重定向 //*.google.cn
^https?:\/\/www\.google\.cn 302 https://www.google.com
^https?:\/\/translate\.google\.cn 302 https://translate.google.com
# PornHub (https://cn.pornhub.com/) //*.pornhub.com
^https:\/\/(?:cn|www)\.pornhub\.com\/_xa\/ads(_batch)?\? reject
# Avple 跳转弹窗 //asserts.avple.tv
^https:\/\/asserts\.avple\.tv\/file\/avple-images\/ad\.js reject

# -----------ddgksf2013----------- #
# PornHub - https://cn.pornhub.com
^https?:\/\/(cn|www)\.pornhub\.com\/_xa\/ads reject-dict

[Script]
# -----------RuCu6----------- #
## 百度 //boxer.baidu.com, m.baidu.com, www.baidu.com, zhidao.baidu.com
## 百度防跳转
http-request ^https:\/\/boxer\.baidu\.com\/scheme\?scheme script-path = https://raw.githubusercontent.com/sooyaaabo/Loon/main/Script/Baidu/BaiduNoRedirect.js, tag = 百度防跳转
## 百度首页
http-response ^https:\/\/(?:www|m)\.baidu\.com(\/|\/\?(tn=&)?from=\d+\w)?$ script-path = https://raw.githubusercontent.com/sooyaaabo/Loon/main/Script/Baidu/BaiduIndex.js, requires-body = true, tag = 百度首页
## 百度知道
http-response ^https:\/\/zhidao\.baidu\.com\/(?:question|index|\?fr|\?word) script-path = https://raw.githubusercontent.com/sooyaaabo/Loon/main/Script/Baidu/BaiduZhidao.js, requires-body = true, tag = 百度知道
# JavDB //javdb.com
http-response ^https:\/\/javdb\.com\/(?!over18\?) script-path = https://raw.githubusercontent.com/sooyaaabo/Loon/main/Script/WebPage/JavDB.js, requires-body = true, tag = JavDB

# -----------ddgksf2013----------- #
